{
  "extends": [
    "react-app",
    "prettier"
  ],
  "rules": {
    "prefer-const": "off",
    "no-use-before-define": ["warn", {"functions": true, "variables": true}],
    "@typescript-eslint/no-unused-vars": [
      "warn",
      {
        "vars": "all",
        "args": "after-used",
        "ignoreRestSiblings": true,
        "varsIgnorePattern": "^_"
      }
    ]
  }
}
