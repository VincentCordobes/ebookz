{
  "extends": [
    "react-app",
    "prettier"
  ],
  "rules": {
    "prefer-const": "off",
    "@typescript-eslint/no-unused-vars": [
      "error",
      {
        "vars": "all",
        "args": "after-used",
        "ignoreRestSiblings": true,
        "varsIgnorePattern": "^_"
      }
    ]
  }
}
